  "bin": "./dist/ota-distribute.js",
  "main": "./dist/ota-distribute.js",
  "devDependencies": {
    "@eslint/js": "^9.16.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/mime-types": "^2",
